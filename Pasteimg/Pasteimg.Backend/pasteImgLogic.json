{
  "Source": {
    "MaxHeight": 2000,
    "MaxWidth": 2000,
    "Quality": 75
  },
  "Storage": {
    "Root": [
      "_wwwimages"
    ],
    "SubDirectoryDivision": 4
  },
  "Thumbnail": {
    "MaxHeight": 300,
    "MaxWidth": 300,
    "Quality": 90
  },
  "Validation": {
    "DescriptionMaxLength": 120,
    "MaxFileSize": 10000000,
    "MaxImagePerUpload": 20,
    "PasswordMaxLength": 12,
    "SupportedFormats": [
      "jpg",
      "jpeg",
      "jpe",
      "jif",
      "jfif",
      "jfi",
      "gif",
      "png",
      "apng",
      "webp",
      "bmp"
    ]
  },
  "Visitor": {
    "LockoutTresholdInMinutes": 10,
    "MaxFailedAttempt": 3
  },
  "Session": {
    "IdleTimeout": "01:00:00",
    "IOTimeout": "00:00:30"
  }
}